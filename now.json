{
  "name": "expo-zeit",
  "version": 2,
  "routes": [{
      "src": "/api/(.*)",
      "dest": "/api/$1"
    },
    {
      "src": "/(.*)",
      "dest": "/web-build/$1"
    }
  ],
  "builds": [{
      "src": "web-build/**/*",
      "use": "@now/static"
    },
    {
      "src": "/api/**",
      "use": "@now/node"
    }
  ],
  "build": {
    "env": {
      "IS_NOW": "true",
      "ENV": "PROD"
    }
  },
  "env": {
    "IS_NOW": "true",
    "ENV": "PROD"
  }
}