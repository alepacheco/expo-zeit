{
  "name": "expo-zeit-test",
  "version": 2,
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "/api/$1"
    },
    {
      "src": "/sockjs-node/(.*)",
      "dest": "ws://localhost:3000/sockjs-node/$1"
    },
    {
      "src": "/(.*)",
      "dest": "/$1"
    }
  ],
  "builds": [
    {
      "src": "package.json",
      "use": "@now/static-build",
      "config": { "distDir": "web-build" }
    },
    {
      "src": "/api/**",
      "use": "@now/node"
    }
  ],
  "build": {
    "env": {
      "IS_NOW": "true",
      "ENV": "PROD"
    }
  },
  "env": {
    "IS_NOW": "true",
    "ENV": "PROD"
  }
}
